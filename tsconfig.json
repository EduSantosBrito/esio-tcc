{
    "compilerOptions": {
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "declaration": true,
        "target": "esnext",
        "moduleResolution": "node",
        "module": "commonjs",
        "outDir": "./dist",
        "allowUnreachableCode": false,
        "allowSyntheticDefaultImports": true,
        "removeComments": true,
        "strict": true,
        "isolatedModules": false,
        "esModuleInterop": true,
        "noImplicitAny": false,
        "jsx": "react",
        "types": [
            "node"
        ]
    },
    "include": [
        "src/**/*"
    ],
    "exclude": []
}